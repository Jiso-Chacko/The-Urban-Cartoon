

<div class="container">


<div class="col-lg-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <table class="table table-hover" id="myTable">
                <thead>
                    <tr>
                        <th ></th>
                        <th >User</th>
                        <th >Email</th>
                        <th >Phone</th>
                        <th >Block/Unblock</th>
                    </tr>
                </thead>
                <tbody>
                    {{#each users}}
                    <tr>
                        <td><i class="mdi mdi-account-circle"></i></td>
                        <td>{{this.userFirstName}}</td>
                        <td>{{this.userEmail}}</td>
                        <td>+91{{this.userPhone}}</td>
                        <td> <a class="btn btn-secondary" style="width: 5.5rem;"
                                onclick="return confirm('Are you sure to disable {{ this.userFirstName }}? ')"
                                href="/admin/disable-user?id={{ this._id }}">Block</a>
                        </td>

                        {{!-- <a class="btn btn-success" style="width: 5.5rem;" onclick="return confirm('Are you sure to enable {{ this.userFirstName }}? ')"  href="/admin/enable-user?id={{ this._id }}">Unblock</a> --}}

                    </tr>
                    {{/each}}
                </tbody>
            </table>
        </div>
    </div>
</div>
</div>




 
   

